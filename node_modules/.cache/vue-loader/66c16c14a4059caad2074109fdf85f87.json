<div class=\"form-groupe mb-5\">\n\t\t\t<label for=\"position\">Entrez le nom d'une ville</label>\n\t\t\t<input type=\"text\" name=\"\" id=\"position\" class=\"form-control\" v:model=\"requete\" v-on:keypress=\"goMeteo\" />\n\t\t</div>\n\n\t\t<div class=\"w-75 m-auto\">\n\t\t\t<h3 class=\"text-center mb-3\">Position : Paris</h3>\n\n\t\t\t<div class=\"card text-center p-5\">\n\t\t\t\t<p class=\"texte-affichage\">Temperature 20</p>\n\n\t\t\t\t<p class=\"texte-affichage\">Temps: Nuageux</p>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n\tname: \"Meteo\",\n\tdata() {\n\t\treturn {\n\t\t\trequete: \"\",\n\t\t\ttemps: undefined,\n\t\t\tapi_code: \"a93612679cb91948ff40c2d5ea5a893b\",\n\t\t\turl_recherche: \"https://api.openweathermap.org/data/2.5/weather?\",\n\t\t};\n\t},\n\tmethods: {\n\t\tgoMeteo(e) {\n\t\t\tif (e.key == \"Enter\") {\n\t\t\t\taxios\n\t\t\t\t\t.get(`${this.url_recherche}q=${this.requete}&units=metric&APPID=${this.api_code}&lang=fr`)\n\t\t\t\t\t.then((reponse) => {\n\t\t\t\t\t\tconsole.log(reponse);\n\t\t\t\t\t});\n\t\t\t}\n\t\t},\n\t},\n};\n</script>\n\n<style>\nh1,\nlabel,\nh3 {\n\tcolor: #f1f1f1;\n\ttext-transform: capitalize;\n\tfont-weight: 700;\n}\n</style>\n"]}]}